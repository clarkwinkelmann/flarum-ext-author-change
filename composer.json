{
    "name": "clarkwinkelmann/flarum-ext-author-change",
    "description": "Let mods update the author and date of discussions and posts",
    "keywords": [
        "flarum",
        "author",
        "date"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2"
    },
    "replace": {
        "clarkwinkelmann/flarum-ext-post-date": "*"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "email": "clark.winkelmann@gmail.com",
            "homepage": "https://clarkwinkelmann.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/clarkwinkelmann/flarum-ext-author-change/issues",
        "source": "https://github.com/clarkwinkelmann/flarum-ext-author-change",
        "forum": "https://discuss.flarum.org/d/21731"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\AuthorChange\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Author Change",
            "category": "feature",
            "icon": {
                "name": "fas fa-user-edit",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": "https://discuss.flarum.org/d/21731"
        }
    }
}
